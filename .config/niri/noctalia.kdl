layout {

    focus-ring {
        active-color   "#ffb1c0"
        inactive-color "#171213"
        urgent-color   "#ffb4ab"
    }

    border {
        active-color   "#ffb1c0"
        inactive-color "#171213"
        urgent-color   "#ffb4ab"
    }

    shadow {
        color "#00000070"
    }

    tab-indicator {
        active-color   "#ffb1c0"
        inactive-color "#7c2940"
        urgent-color   "#ffb4ab"
    }

    insert-hint {
        color "#ffb1c080"
    }
}

recent-windows {
    highlight {
        active-color "#ffb1c0"
        urgent-color "#ffb4ab"
    }
}
